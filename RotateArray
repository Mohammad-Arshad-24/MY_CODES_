  int l=sizeof(arr)/sizeof(arr[0]);
    for(int j=0;j<d;j++){  //Here 'd' is the no.of rotations!!!!
        
    for(int i=0;i<l-1;i++){
        if(i==0){
            int temp=arr[l-1];
            arr[l-1]=arr[i];
            arr[i]=temp;}
        
        else{
        int temp=arr[i-1];
        arr[i-1]=arr[i];
        arr[i]=temp;}
        
        // printf("%d   ",arr[i]);
    }}
    for(int i=0;i<l;i++){
    printf("%d ",arr[i]);}

    return 0;
