class Solution {
    public int[] productExceptSelf(int[] nums) {
        int[] res=new int[nums.length];
        int h=0;
        
        for(int i=0;i<nums.length;i++){
        int product = 1;
                for(int j=0;j<nums.length;j++){
                    if(j==h){
                        continue;
                    }
                        product*=nums[j];
                }
                res[i]=product;
                h++;
            }
            return res;
            }
}      
        
    
