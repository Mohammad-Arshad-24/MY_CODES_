class Solution:
    def findMajority(self, arr):
        na = set(arr)
        na = list(na)
        n = len(arr)
        m = len(na)
        a = dict()
        for i in range(m):
            c = 0
            for j in range(n):
                if na[i] == arr[j]:
                    c += 1
            a[na[i]] = c
        arr = []
        for keys in a.keys():
            if a[keys] > (n / 3):
                arr.append(keys)
        return sorted(arr)
