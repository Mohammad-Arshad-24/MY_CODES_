#include<iostream>

using namespace std;
int main() {
    // Write C++ code here
    int n=5,i,j;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n-i;j++)
        {
            cout<<" ";
        }
        for(j=i;j>0;j--)
        {
            cout<<" "<<j ;
        }
        cout<<endl;
    }
    return 0;
}
